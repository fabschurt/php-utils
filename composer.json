{
  "name": "fabschurt/php-utils",
  "description": "A collection of common PHP utility classes.",
  "type": "library",
  "homepage": "https://github.com/fabschurt/php-utils",
  "keywords": ["php", "utility", "tools"],
  "license": "MIT",
  "authors": [
    {
      "name": "Fabien Schurter",
      "email": "fabien@fabschurt.com",
      "homepage": "http://fabschurt.com/",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "dev@fabschurt.com",
    "issues": "https://github.com/fabschurt/php-utils/issues",
    "source": "https://github.com/fabschurt/php-utils/tree/master",
    "docs": "https://github.com/fabschurt/php-utils/blob/master/README.md"
  },
  "require": {
    "php": "^5.6|^7.0",
    "danielstjules/stringy": "^2.3",
    "vlucas/phpdotenv": "^2.4",
    "symfony/filesystem": "^2.8|^3.0"
  },
  "require-dev": {
    "consolidation/robo": "^1.0",
    "friendsofphp/php-cs-fixer": "^1.12",
    "peridot-php/peridot": "^1.18",
    "peridot-php/leo": "^1.6",
    "mikey179/vfsStream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "FabSchurt\\Php\\Utils\\": "src/"
    }
  },
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
